#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/macbook/Desktop/freelancer/mono/fs-turbo/node_modules/.pnpm/@bufbuild+buf@1.32.2/node_modules/@bufbuild/buf/bin/node_modules:/Users/macbook/Desktop/freelancer/mono/fs-turbo/node_modules/.pnpm/@bufbuild+buf@1.32.2/node_modules/@bufbuild/buf/node_modules:/Users/macbook/Desktop/freelancer/mono/fs-turbo/node_modules/.pnpm/@bufbuild+buf@1.32.2/node_modules/@bufbuild/node_modules:/Users/macbook/Desktop/freelancer/mono/fs-turbo/node_modules/.pnpm/@bufbuild+buf@1.32.2/node_modules:/Users/macbook/Desktop/freelancer/mono/fs-turbo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/macbook/Desktop/freelancer/mono/fs-turbo/node_modules/.pnpm/@bufbuild+buf@1.32.2/node_modules/@bufbuild/buf/bin/node_modules:/Users/macbook/Desktop/freelancer/mono/fs-turbo/node_modules/.pnpm/@bufbuild+buf@1.32.2/node_modules/@bufbuild/buf/node_modules:/Users/macbook/Desktop/freelancer/mono/fs-turbo/node_modules/.pnpm/@bufbuild+buf@1.32.2/node_modules/@bufbuild/node_modules:/Users/macbook/Desktop/freelancer/mono/fs-turbo/node_modules/.pnpm/@bufbuild+buf@1.32.2/node_modules:/Users/macbook/Desktop/freelancer/mono/fs-turbo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@bufbuild/buf/bin/protoc-gen-buf-lint" "$@"
else
  exec node  "$basedir/../@bufbuild/buf/bin/protoc-gen-buf-lint" "$@"
fi
